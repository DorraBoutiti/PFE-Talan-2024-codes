{
  "name": "HrDocumentAnalyzer",
  "version": "1.0.0",
  "description": "Dockerized environment with a database, a back-end, and a front-end application.",
  "scripts": {
    "build:back": "docker-compose build backend-document-analyzer",
    "build:front": "docker-compose build frontend-document-analyzer",
    "build:upload": "docker-compose build module-upload",
    "build:uploadF": "docker-compose build module_upload_fastapi",
    "build:stt": "docker-compose build module-stt",
    "build:sirh": "docker-compose build module-sirh",
    "build:image": "docker-compose build module_image_enhancement",
    "build:imageF": "docker-compose build module_image_enhancement_fastapi",
    "build:extract": "docker-compose build module_extract_data",
    "build:classif": "docker-compose build module-classification",
    "build:ocr": "docker-compose build module_ocr",
    "build:cypress": "docker-compose build cypress",
    "build:openhr": "docker-compose build module_conn_sirh",
    "build:all": "docker-compose build",

    "start:db": "docker-compose up -d mysql-db",
    "start:back": "docker-compose up -d backend-document-analyzer",
    "start:front": "docker-compose up -d frontend-document-analyzer",
    "start:upload": "docker-compose up -d module-upload",
    "start:uploadF": "docker-compose up -d module_upload_fastapi",
    "start:stt": "docker-compose up -d module-stt",
    "start:sirh": "docker-compose up -d module-sirh",
    "start:image": "docker-compose up -d module_image_enhancement",
    "start:imageF": "docker-compose up -d module_image_enhancement_fastapi",
    "start:extract": "docker-compose up -d module_extract_data",
    "start:classif": "docker-compose up -d module-classification",
    "start:ocr": "docker-compose up -d module_ocr",
    "start:cypress": "docker-compose up cypress",
    "start:openhr": "docker-compose up module_conn_sirh",
    "start:all": "docker-compose up -d --build",
    "clean": "docker-compose down -v --remove-orphans && docker image prune -af",
    "restart:back": "docker-compose up -d --build backend-document-analyzer",
    "restart:front": "docker-compose up -d --build frontend-document-analyzer",
    "restart:upload": "docker-compose up -d --build module-upload",
    "restart:uploadF": "docker-compose up -d --build module_upload_fastapi",
    "restart:stt": "docker-compose up -d --build module-stt",
    "restart:sirh": "docker-compose up -d --build module-sirh",
    "restart:image": "docker-compose up -d --build module_image_enhancement",
    "restart:imageF": "docker-compose up -d --build module_image_enhancement_fastapi",
    "restart:extract": "docker-compose up -d --build module_extract_data",
    "restart:classif": "docker-compose up -d --build module_classification",
    "restart:ocr": "docker-compose up -d --build module_ocr",
    "restart:cypress": "docker-compose up -d --build cypress"
    
  },
  "author": "Talan Consulting Tunisie",
  "license": "ISC"
}
